{
  "name": "@itsmarathon/eqcadt-models",
  "version": "1.0.2",
  "description": "Open source ONNX models for conversational AI - optimized from facebook/bart-large-mnli",
  "keywords": [
    "onnx",
    "bart",
    "nli",
    "natural-language-inference",
    "intent-detection",
    "conversational-ai",
    "machine-learning",
    "web-ml"
  ],
  "homepage": "https://github.com/itsmarathon/eqcadt-models#readme",
  "bugs": {
    "url": "https://github.com/itsmarathon/eqcadt-models/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsmarathon/eqcadt-models.git"
  },
  "license": "MIT",
  "author": "EQCADT Team",
  "type": "commonjs",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "index.js",
    "models/chunks/",
    "models/model-manifest.json",
    "scripts/model-assembler.js",
    "scripts/verify-integrity.js",
    "docs/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "verify": "node scripts/verify-integrity.js",
    "assemble": "node scripts/model-assembler.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run verify && npm test",
    "size": "du -sh models/*",
    "clean": "rm -f models/bart-nli-web.onnx"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "jest": "^29.5.0",
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
